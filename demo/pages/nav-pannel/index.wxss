.page {
  height: 100vh;
  background-color: #f2f2f2;
  padding: 0 30rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

.page .pannel {
  margin-bottom: 30rpx;
  display: flex;
  flex-direction: column;
  flex: 1;
}

.page .pannel .pannel-inner {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.page .pannel .pannel-inner .nav-pannel {
  flex: 1;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #cec6c6;
}

.page .pannel .pannel-inner .nav-pannel button {
  background-color: #d13435;
  color: #fff;
  font-size: 28rpx;
  margin-top: 28rpx;
}

.page .pannel .pannel-inner .nav-pannel button:last-child {
  background-color: #fff;
  color: #d13435;
}
